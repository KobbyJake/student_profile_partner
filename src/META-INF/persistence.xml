<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ProPartnerPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>model.Programme</class>
    <class>model.PasswordResets</class>
    <class>model.Location</class>
    <class>model.CourseRegistry</class>
    <class>model.Schedule</class>
    <class>model.CourseRead</class>
    <class>model.Student</class>
    <class>model.Document</class>
    <class>model.GradingScheme</class>
    <class>model.Honors</class>
    <class>model.Semester</class>
    <class>model.Event</class>
    <class>model.System</class>
    <class>model.System_1</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:sqlite:lib\propartDB.db"/>
      <property name="javax.persistence.jdbc.user" value=""/>
      <property name="javax.persistence.jdbc.driver" value="org.sqlite.JDBC"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
